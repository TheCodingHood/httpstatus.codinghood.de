{% extends 'base.html.twig' %}


{% block body %}
    Here are all the codes we support (and any special notes):<br/>
    {% for code, statusCode in statusCodes %}
        <a href="{{ path("status", {'status_code': code}) }}">{{ code }}</a> - {{ statusCode.description }}

        {% if statusCode.no_standard is defined and statusCode.no_standard %}
            (non-standard status code)
        {% endif %}

        <br/>
    {% endfor %}
{% endblock %}
